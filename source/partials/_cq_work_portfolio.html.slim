.wrapper.padding-none
  .m-cq-work-portfolio.m-grid-collection

    .m-grid-element.small.foundersuite.bcg-companies-foundersuite
      .logo.large
      = partial 'partials/cq_work_portfolio_content', locals: \
        {element: data.foundersuite}
        = partial 'svg/work_logos/foundersuite'

    .m-grid-element.large.testimonial.light-grey-bcg
      = partial 'partials/cq_work_portfolio_testimonial', locals: \
      {quote: "We couldn't have achieved the success we've had without them. \
       <p>We look forward to many more years of working together.</p>",
       name: 'Nathan Beckord,',
       position: '&nbsp;Co-founder and CEO at Foundersuite',
       quote_color: '', author_color: '', position_color: ''}

    .m-grid-element.large.dawanda.bcg-companies-dawanda
      .logo.large.tablet-small
        = partial 'svg/work_logos/dawanda'
      = partial 'partials/cq_work_portfolio_content', locals: \
        {element: data.dawanda}

    .m-grid-element.small.eqipia.bcg-companies-eqipia
      .logo.x-small.tablet-x-small
        = partial 'svg/work_logos/eqipia'
      = partial 'partials/cq_work_portfolio_content', locals: \
          {element: data.eqipia}

    .m-grid-element.large.testimonial.damask-red-bcg.font-light-grey
      = partial 'partials/cq_work_portfolio_testimonial', locals: \
       {quote: "Professionalism and enthusiasm are the main qualities of code \
        quest engineers. They have been up to the challenge and have \
        worked with our team shoulder to shoulder to ensure the \
        success of our project.",
        name: 'Luis Hernando Blanco,',
        position: '&nbsp;Co-founder and VP at SelectHub',
        quote_color: 'valencia-red',
        author_color: 'font-martinique-dark',
        position_color: 'light-grey'}

    .m-grid-element.small.selecthub.bcg-companies-selecthub
